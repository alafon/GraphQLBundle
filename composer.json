{
    "name": "overblog/graphql-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "This bundle provides tools to build a GraphQL server in your Symfony App.",
    "keywords": ["GraphQL", "Relay"],
    "authors": [
        {
            "name": "Overblog",
            "homepage": "http://www.over-blog.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Overblog\\GraphQLBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "files": [
            "vendor/overblog/graphql-php-generator/tests/DateTimeType.php",
            "vendor/overblog/graphql-php-generator/tests/Resolver.php"
        ]
    },
    "config" : {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": "~7.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "overblog/graphql-php-generator": "^0.4.1",
        "symfony/expression-language": "^3.1",
        "symfony/framework-bundle": "^3.1",
        "symfony/options-resolver": "^3.1",
        "symfony/property-access": "^3.1",
        "webonyx/graphql-php": "^0.7"
    },
    "suggest": {
        "twig/twig": "If you want to use graphiQL."
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^5.5",
        "symfony/phpunit-bridge": "^3.1",
        "sensio/framework-extra-bundle": "^3.0",
        "sllh/php-cs-fixer-styleci-bridge": "^2.1.1",
        "symfony/asset": "^3.1",
        "symfony/browser-kit": "^3.1",
        "symfony/css-selector": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "symfony/security-bundle": "^3.1",
        "symfony/templating": "^3.1",
        "symfony/twig-bundle": "^3.1",
        "symfony/web-profiler-bundle": "^3.1",
        "symfony/yaml": "^3.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.7-dev"
      }
    }
}
